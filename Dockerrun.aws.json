{
	"AWSEBDockerrunVersion": 1,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "caihedoro/multi-client",
			"hostname": "client",
			"essential": false,
			"memory": 128
		},
		{
			"name": "api",
			"image": "caihedoro/multi-server",
			"hostname": "api",
			"essential": false,
			"memory": 128
		},
		{
			"name": "worker",
			"image": "caihedoro/multi-worker",
			"essential": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "caihedoro/multi-nginx",
			"essential": true,
			"portMappings": [{
				"hostPort": 80,
				"containerPort": 80
			}],
			"links": ["client", "api"],
			"memory": 128
		}
	]
}